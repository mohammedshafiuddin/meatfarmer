{
  "expo": {
    "name": "Freshyo",
    "slug": "freshyo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_mini.jpg",
    "scheme": "freshyo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "in.freshyo.app",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "ppemerchantsdkv1",
          "ppemerchantsdkv2",
          "ppemerchantsdkv3",
          "paytmmp",
          "gpay",
          "ppemerchantsdkv1",
          "ppemerchantsdkv2",
          "ppemerchantsdkv3",
          "paytmmp",
          "gpay",
          "ppemerchantsdkv1",
          "ppemerchantsdkv2",
          "ppemerchantsdkv3",
          "paytmmp",
          "gpay",
          "ppemerchantsdkv1",
          "ppemerchantsdkv2",
          "ppemerchantsdkv3",
          "paytmmp",
          "gpay"
        ]
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_mini.jpg",
        "backgroundColor": "#fff0f6"
      },
      "edgeToEdgeEnabled": true,
      "package": "in.freshyo.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/symbuyote.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7f3e7611-f7a8-45f9-8a99-c2b1f6454d48"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
