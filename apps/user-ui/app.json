{
  "expo": {
    "name": "Symbuyote",
    "slug": "locality",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/symbuyote.png",
    "scheme": "medikore",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mohammedshafiudin54.locality-user-ui",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "ppemerchantsdkv1",
          "ppemerchantsdkv2",
          "ppemerchantsdkv3",
          "paytmmp",
          "gpay",
          "ppemerchantsdkv1",
          "ppemerchantsdkv2",
          "ppemerchantsdkv3",
          "paytmmp",
          "gpay"
        ]
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/symbuyote.png",
        "backgroundColor": "#fff0f6"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mohammedshafiuddin54.locality"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/symbuyote.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fd509174-c857-4f98-a53c-9cd4f07d6dea"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fd509174-c857-4f98-a53c-9cd4f07d6dea"
    }
  }
}
